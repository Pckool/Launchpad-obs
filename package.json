{
  "name": "launchpad-obs",
  "productName": "Launchpad OBS",
  "version": "1.0.0",
  "description": "An application used to connect the novation launchpad to obs.",
  "main": "src/index.js",
  "repository": "https://github.com/Pckool/Launchpad-obs",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color"
  },
  "keywords": [],
  "author": "phill",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "./src/media/images/lp-obs.ico",
        "asar": true
      },
      "electronWinstallerConfig": {
        "name": "LaunchpadOBS",
        "setupIcon": "./src/media/images/lp-obs.ico"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "Pckool",
        "name": "Launchpad-obs"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "launchpadobs"
      }
    }
  },
  "dependencies": {
    "easymidi": "^1.0.1",
    "electron-compile": "^6.4.4",
    "electron-squirrel-startup": "^1.0.0",
    "fontawesome": "^5.6.3",
    "generic-midi-controller": "^1.0.3",
    "i": "^0.3.6",
    "jquery": "^3.3.1",
    "node-abi": "^2.7.1",
    "npm": "^6.13.4",
    "obs-websocket-js": "^2.0.1",
    "opn": "^5.4.0",
    "path": "^0.12.7",
    "ws": "^6.2.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.12.4"
  }
}
